var votes = document.querySelector("#link-<%= @link.id %> .votes");
votes.querySelector(".link-points").innerText = <%= @link.points %>;
votes.querySelectorAll(".upvote, .downvote").forEach(element => element.classList.remove("active"));

<% if @new_vote %>
  votes.querySelector(".downvote").classList.add("active");
<% end %>
